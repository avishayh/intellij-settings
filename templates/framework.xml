<templateSet group="framework">
  <template name="format" value="MessageUtil.format($MESSAGE$)" description="MessageUtil.format" toReformat="false" toShortenFQNames="true">
    <variable name="MESSAGE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="COMPLETION" value="false" />
      <option name="JAVA_CODE" value="true" />
      <option name="JAVA_COMMENT" value="false" />
      <option name="JAVA_STRING" value="false" />
    </context>
  </template>
  <template name="noe" value="StringUtils.isNullOrEmpty($STRING$)" description="is null or empty String" toReformat="false" toShortenFQNames="true">
    <variable name="STRING" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="COMPLETION" value="false" />
      <option name="JAVA_CODE" value="true" />
      <option name="JAVA_COMMENT" value="false" />
      <option name="JAVA_STRING" value="false" />
    </context>
  </template>
  <template name="get" value="com.idi.framework.common.environment.Application.getInstance($TYPE$.class);" description="" toReformat="true" toShortenFQNames="true">
    <variable name="TYPE" expression="expectedType()" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="GROOVY" value="true" />
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="sget" value="Application.getInstance(SpringApplication.class).get($TYPE$.class);" description="Application gets SpringApplication" toReformat="true" toShortenFQNames="true">
    <variable name="TYPE" expression="expectedType()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="GROOVY" value="true" />
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="mock" value="org.mockito.Mockito.mock($TYPE$.class);" description="Mockito create Mock" toReformat="false" toShortenFQNames="true">
    <variable name="TYPE" expression="expectedType()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="ssget" value="com.idi.framework.common.environment.Application.getInstance(SpringService.class).get($TYPE$.class);" description="Application gets SpringApplication" toReformat="true" toShortenFQNames="true">
    <variable name="TYPE" expression="expectedType()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="sget2" value="Application.getInstance(SpringApplication.class).get($NAME$);" description="Application gets SpringApplication" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>